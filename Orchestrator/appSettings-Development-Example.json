{
  "ConnectionStrings": {
    "PostgreSQLConnection": "Server=localhost;Port=5452;Database=postgres;User Id=postgres;Password=dlcs_password;Command Timeout=60;"
  },
  "AWS": {
    "Profile": "default",
    "Region": "eu-west-1",
    "UseLocalStack": true,
    "S3": {
      "OutputBucket": "dlcs-output",
      "ThumbsBucket": "dlcs-thumbs"
    }
  },
  "Proxy": {
    "S3HttpBase": "http://localhost:4566",
    "StorageBucket": "dlcs-storage",
    "ImageServerRoot": "http://localhost:5025",
    "CanResizeThumbs": true,
    "ThumbUpscaleConfig": {
      "CanUpsize": {
        "AssetIdRegex": "2/1/upsize.*",
        "UpscaleThreshold": 100
      }
    }
  },
  "RunMigrations": true,
  "S3OriginRegex": "http\\\\:\\\\/\\\\/localhost:4566\\\\/.*",
  "AuthServicesUriTemplate": "https://localhost:5003/auth/{customer}/{behaviour}",
  "ImageFolderTemplateImageServer": "/nas/{customer}/{space}/{image-dir}/{image}.jp2",
  "ImageFolderTemplateOrchestrator": "/nas/{customer}/{space}/{image-dir}/{image}.jp2",
  "ApiSalt": "a-secret",
  "ApiRoot": "TODO",
  "Auth": {
    "CookieDomains": [
      "localhost",
      "127.0.0.1"
    ]
  },
  "Caching": {
    "TimeToLive": {
      "Memory": {
        "ShortTtlSecs": 60,
        "DefaultTtlSecs": 90,
        "LongTtlSecs": 120,
        "Overrides": {
          "InfoJsonTtlSecs": 60
        }
      },
      "Http": {
        "ShortTtlSecs": 60,
        "DefaultTtlSecs": 90,
        "LongTtlSecs": 120
      }
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "image_server": {
        "Destinations": {
          "image_server/one": {
            "Address": "http://localhost:5025"
          }
        }
      },
      "thumbs": {
        "Destinations": {
          "thumbs/one": {
            "Address": "http://localhost:5019"
          }
        }
      },
      "thumbresize": {
        "Destinations": {
          "thumbresize/one": {
            "Address": "http://localhost:5019"
          }
        }
      }
    }
  }
}
