{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Orchestrator"
    }
  },
  "Proxy": {
    "ThumbsPath": "thumbs",
    "ImagePath": "iiif-img",
    "CanResizeThumbs": true
  },
  "Caching": {
    "TimeToLive": {
      "Memory": {
        "ShortTtlSecs": 60,
        "DefaultTtlSecs": 600,
        "LongTtlSecs": 1800
      },
      "Http": {
        "ShortTtlSecs": 21600,
        "DefaultTtlSecs": 86400,
        "LongTtlSecs": 604800
      }
    }
  },
  "OrchestrateOnInfoJson": true,
  "ReverseProxy": {
    "Routes": {
      "img_options": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-img/{customer}/{space}/{image}/{**assetRequest}",
          "Methods": [
            "OPTIONS"
          ]
        }
      },
      "av_infojson": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-av/{cust}/{space}/{image}/info.json",
          "Methods": [
            "GET"
          ]
        }
      },
      "av_only": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-av/{cust}/{space}/{image}",
          "Methods": [
            "GET"
          ]
        }
      },
      "thumbs": {
        "ClusterId": "thumbs",
        "Match": {
          "Path": "/thumbs/{**assetRequest}",
          "Methods": [
            "GET"
          ]
        }
      },
      "av_options": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/iiif-av/{customer}/{space}/{image}/{**assetRequest}",
          "Methods": [
            "OPTIONS"
          ]
        }
      },
      "fallback": {
        "ClusterId": "deliverator",
        "Match": {
          "Path": "/{**root:regex(^(?!.*(iiif-img|iiif-av|file|iiif-manifest|auth|iiif-resource|pdf\/)).*$)}"
        }
      }
    }
  },
  "AllowedHosts": "*",
  "PathBase": "",
  "CriticalPathTimeoutMs": 10000
}