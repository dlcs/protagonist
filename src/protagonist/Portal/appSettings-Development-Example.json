{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Portal"
    }
  },
  "ConnectionStrings": {
    "PostgreSQLConnection": "<connection-string>"
  },
  "AWS": {
    "Profile": "default",
    "Region": "eu-west-1",
    "S3": {
      "OriginBucket": "<storage-origin-bucket>"
    }
  },
  "Portal": {
    "loginSalt": "a-salt-value"
  },
  "API": {
    "PageSize": 100
  },
  "DLCS": {
    "ApiRoot": "https://api.dlcs"
  }
}
